<Window x:Class="_2017Solver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="Start" Content="Add to dictionary" Margin="375,289,10,0" Click="Button_Click" Height="20" VerticalAlignment="Top"/>
        <TextBox x:Name="Input" HorizontalAlignment="Left" Margin="10,286,0,0" TextWrapping="Wrap" Width="290" Height="23" VerticalAlignment="Top"/>
        <DataGrid x:Name="Result" Margin="10,10,10,0" IsManipulationEnabled="True" SelectionChanged="Result_SelectionChanged" Height="271" VerticalAlignment="Top"/>

    </Grid>
</Window>
